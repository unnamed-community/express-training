{
  "parserOptions": {
    "ecmaVersion": 9,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "rules": {
    "semi": ["error", "never"],
    "valid-jsdoc": 1,
    "require-jsdoc": 1,
    "no-duplicate-imports": 2,
    "require-atomic-updates": 0,
    "no-console": 2,
    "quotes": ["error", "single"]
  }
}